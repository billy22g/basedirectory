<%= form_for(@base) do |f| %>
  <%= render 'shared/errors' %>
  <p>
  <%= f.label :name %><br/>
  <%= f.text_field :name, autofocus: true %>
  </p>
  <p>
  <%= f.label :branch %><br/>
  <%= f.text_field :branch %>
  </p>
  <p>
  <%= f.label :city %><br/>
  <%= f.text_field :city %>
  </p>
  <p>
  <%= f.label :state %><br/>
  <%= f.text_field :state %>
  </p>
  <p>
  <%= f.label :lat %><br/>
  <%= f.text_field :lat %>
  </p>
  <p>
  <%= f.label :long %><br/>
  <%= f.text_field :long %>  
  </p>
  <p>
  <%= f.label :image_filename %><br/>
  <%= f.text_field :image_filename, size: 30, placeholder: "GIF, JPG, or PNG file" %>  
  </p>
  <%= f.submit %>
<% end %><br/><br/>
